  "author": {
    "tg": "TitProger",
    "url": "https://github.com/TitProger",
    "name": "Tit' Mirgaliev"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/postgresql": "^6.2.9",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "pg": "^8.16.3"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "npx mikro-orm migration:create --config src/common/config/mikro-orm/mikro-orm.config.ts --name ",
    "migration:list": "npx mikro-orm migration:list --config src/common/config/mikro-orm/mikro-orm.config.ts",
    "migration:up": "npx mikro-orm migration:up --config src/common/config/mikro-orm/mikro-orm.config.ts",
    "migration:down": "npx mikro-orm migration:down --config src/common/config/mikro-orm/mikro-orm.config.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.2.9",
    "@nestjs/cli": "^10.4.9",
    "ts-node": "^10.9.2"
  }
}
